}

ol, ul {
  list-style: none;
}

a, a:visited {
  text-decoration: none;
}

:focus {
  outline: 0;
  border: 0;
  -webkit-appearance: none;
}
.clearfix:after { 
  content: "." ; 
  display: block ;
  height: 0 ;
  clear: both ;
  visibility: hidden ;
  }

