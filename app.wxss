/* switch */
.switch{
    position: relative;
    width:80rpx ;
    height:42rpx ;
    background: #cccccc ;
    border: none ;
    z-index: 10;
    display: inline-block;
    outline: medium;
    border-radius: 21rpx;
    -webkit-appearance: none;
    -moz-appearance: none;
    vertical-align: middle;
}
.switch-node{
  top: 2rpx;
  left: 2rpx;
  z-index: 1;
  width: 38rpx;
  height: 38rpx;
  -webkit-transition: 0.3s;
  transition: 0.3s;
  position: absolute;
  border-radius: 38rpx;
  padding: 0 ;
  background-color: #fff;
  border: none;
  min-height: 38rpx;
  box-shadow: 0 3px 1px 0 rgba(0, 0, 0, 0.05), 0 2px 2px 0 rgba(0, 0, 0, 0.1), 0 3px 3px 0 rgba(0, 0, 0, 0.05);
}
.switch.on {
  background-color: #f4393c;
}
.switch.on .switch-node{
  right: 2rpx;
  left: auto;
}
