/*表单元素  start*/

button, input[type="button"], input[type="reset"], input[type="submit"] {
  -webkit-appearance: button;
  cursor: pointer;
}

input:focus, input[type="password"] {
  outline: none;
  border: 0;
  -webkit-appearance: none;
}

input[type="search"] {
  -webkit-appearance: none;
}

input::-webkit-search-decoration, input::-webkit-search-cancel-button {
  display: none;
}

textarea {
  overflow: auto;
  resize: none;
}

input, textarea, select, :checked {
  font-size: 1em;
  border: 0;
  line-height: 1em;
  margin: 0;
  padding: 0;
  -webkit-appearance: none;
  background-color: rgba(0, 0, 0, 0);
}

input:disabled {
  opacity: 1;
}

input[type="radio"] {
  -webkit-border-radius: 0;
}

input + del {
  width: 1.125em;
  height: 1.125em;
  background: url(http://www.51jct.cn/weixin/miniprogram/clear.png) no-repeat center;
  -webkit-background-size: 100% 100%;
  outline: none;
}

input:focus:valid + del {
  display: block;
}

 input, textarea {
  margin: 0px;
  border: 0px;
  width: 100%;
  height: 100%;
  -webkit-appearance: none;
  line-height: 1.2em;
  text-decoration: none;
  font-size: 1em;
  display: block;
  background-color: #fff;
} 

/*表单元素  end*/

img {
  -webkit-transition-duration: 500ms;
  opacity: 1;
  border: 0;
}

img.opacity, .opacity {
  opacity: 0;
}

.img-no-repeat {
  -webkit-background-size: contain;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center;
}

.img-no-repeat-cover {
  -webkit-background-size: cover;
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
}

pre {
  overflow: hidden;
  white-space: pre-wrap; /*保留空白，进行换行*/
