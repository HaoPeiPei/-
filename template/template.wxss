.qrcodeModal{
  position: fixed;
  top: 0rpx;
  bottom: 0rpx;
  background-color: #3f3a3a;
  width: 100%;
  z-index: 999;
}
.pop-full {
  background-color: #fff;
  border-radius: 15rpx;
  margin:auto;
  width:80%;
  position: relative;
  overflow: visible;
}
.pop-full-close {
  position: absolute;
  top: -75rpx;
  right: 47%;
  width: 62rpx;
  height: 62rpx;
  border-radius: 50%;
  background-color: transparent;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100%;
}
.pop-full .pop-content {
  overflow: auto;
  background-color: #fff;
  border-radius: 15rpx;
  -webkit-overflow-scrolling: touch;
}
.pop-full .pop-content .title{
  margin: 0rem;
  padding: 30rpx 0rem;
  text-align: center;
  border-bottom: 1rpx solid #d7d7d7;
}
.pop-full .pop-content .title p.active{
  border-bottom: solid 1 #f4393c;
}
.pop-full .pop-content .content {
  padding:1.5rem 0rem;
  margin: 0rem;
}
.pop-full .pop-content .content {
  padding:30rpx 20rpx;
  color:#333;
}
.pop-full .pop-content .content .wxParse-p {
  overflow:hidden;
}
