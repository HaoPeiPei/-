<!--pages/dpc/dpc_qzf/dpc_qzf.wxml-->
<import src="../../../template/template.wxml" />
<template is="header_content_noBackod" data="{{...header_text}}" />
<view class='content'>
  <view class='list'>
    <view class='li-view margin-l-5 margin-r-5'>
      <view class='box box-f1 box-ac '>
        <view class='logo-text box box-hor'>
          <image src='../../images/dh-b.png'></image>
          <view class='box box-ac font-15 margin-l-2'>代泊车</view>
        </view>
        <view class='box box-f1 box-ac box-pe font-2'>购买日期 2017-08-16 18:30:45</view>
      </view>
    </view>
    <view class='li-view margin-l-5 margin-r-5 no-border'>
      <view class='box box-f1 box-ac box-hor'>
        <view class='box box-ac'>
          <view class='img-text'>
            <image src='../../images/valet_bg.png'></image>
          </view>
        </view>
        <view class='box box-f1 box-ver font-1' style='height:240rpx;margin-left:30rpx'>
          <view class='box box-f1 box-hor box-ac'>
            <view class='dd-text'>订单状态</view>
            <view class='box box-f1 box-ac color-z'>待支付</view>
          </view>
          <view class='box box-f1 box-hor box-ac'>
            <view class='dd-text'>订单号</view>
            <view class='box box-f1 box-ac color-z'>V201708061000020</view>
          </view>
          <view class='box box-f1 box-hor box-ac'>
            <view class='dd-text'>支付金额</view>
            <view class='box box-f1 box-ac  color-z'>￥175</view>
          </view>
        </view>
      </view>
    </view>
    <view class='bg-ee' style='width:100%;height:10rpx'></view>
    <view class='li-view margin-l-5 margin-r-5'>
      <view class='box box-f1 box-ac'>
        <view class='box box-apc'>
          <view class='ddxq-icon'>
            <image src='../../images/dd-9.png'></image>
          </view>
        </view>
        <view class='box box-f1 box-ac font-1 margin-l-2 color-9'>订单详情</view>
      </view>
    </view>
    <block wx:for="{{arrays}}">
      <view class='li-view margin-l-5 margin-r-5 no-border'>
        <view class='box box-f1'>
          <view class='loback '>{{item.title}}</view>
          <view class='box box-f1 box-ac font-1'>{{item.dataStart}}</view>
        </view>
      </view>
    </block>
    <view class='bg-ee' style='width:100%;height:10rpx'></view>
    <view class='li-view margin-l-5 margin-r-5'>
      <view class='box box-f1 box-ac'>
        <view class='box box-apc'>
          <view class='ddxq-icon'>
            <image src='../../images/dd-9.png'></image>
          </view>
        </view>
        <view class='box box-f1 box-ac font-1 margin-l-2 color-9'>车辆详情</view>
      </view>
    </view>
    <block wx:for="{{clxx}}">
      <view class='li-view margin-l-5 margin-r-5 no-border'>
        <view class='box box-f1'>
          <view class='loback '>{{item.title}}</view>
          <view class='box box-f1 box-ac font-1'>{{item.dataVehicle}}</view>
        </view>
      </view>
    </block>
    <view class='footer-zf'>
      <view class='box box-f1 box-hor'>
        <view class='box box-f1 box-apc qxdd-btn'>
          取消订单
        </view>
        <view class='box box-f1 box-apc ljzf-btn'>
          立即支付
        </view>
      </view>
    </view>
  </view>
</view>
<view></view>