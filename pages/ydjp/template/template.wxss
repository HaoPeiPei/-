.endorseModal{
    position: fixed;
    top: 0rpx;
    bottom: 0rpx;
    background-color: #3f3a3a;
    width: 100%;
    z-index: 999;
}
.endorseModal .pop-full {
    background-color: #fff;
    border-radius: 15rpx;
    margin:auto;
    width:70%;
    position: relative;
    overflow: visible;
}
.endorseModal .pop-full-close {
    position: absolute;
    top: -60rpx;
    right: 47%;
    width: 40rpx;
    height: 40rpx;
    border: 1px solid #fff;
    border-radius: 50%;
    background-color: transparent;
    overflow: visible;
}
.endorseModal .pop-full-close:after {
    content: '';
    position: absolute;
    top: 40rpx;
    left: 50%;
    width: 1rpx;
    height: 20rpx;
    background-color: #fff;
    -webkit-transform: translateX(-50%);
    transform: translateX(-50%);
} 
.endorseModal .pop-full-close i {
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
}
.endorseModal .pop-full-close i:before {
    -webkit-transform: translate(-50%,-50%) rotate(45deg);
    transform: translate(-50%,-50%) rotate(45deg);
}
.endorseModal .pop-full-close i:after {
    -webkit-transform: translate(-50%,-50%) rotate(-45deg);
    transform: translate(-50%,-50%) rotate(-45deg);
}
.endorseModal .pop-full-close i:after, .pop-full-close i:before {
    content: '';
    position: absolute;
    top: 50%;
    width: 20rpx;
    height: 1rpx;
    background-color: #fff;
    left: 50%;
}
.endorseModal .pop-full .pop-content {
    overflow: auto;
    background-color: #fff;
    border-radius: 15rpx;
    -webkit-overflow-scrolling: touch;
}
.endorseModal .pop-full .pop-content .title{
    margin: 0rem;
    padding: 30rpx 0rem;
    text-align: center;
    border-bottom: 1rpx solid #d7d7d7;
}
.endorseModal .pop-full .pop-content .title p.active{
    border-bottom: solid 1 #f4393c;
}
.endorseModal .pop-full .pop-content .content {
    padding:1.5rem 0rem;
    margin: 0rem;
}
.endorseModal .pop-full .pop-content .content {
    padding:30rpx 20rpx;
    color:#333;
}
.endorseModal .pop-full .pop-content .content .wxParse-p {
    overflow:hidden;
}

