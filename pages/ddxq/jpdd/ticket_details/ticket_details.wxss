.popup-class{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
}
.popup-content{
  width: 85%;
  background: #ffffff;
  border-radius: 10rpx;
}
