.endorseModal, .div_mulLine, .priceDetailModal{
  position: fixed;
  top: 0rpx;
  bottom: 0rpx;
  background-color: #3f3a3a;
  width: 100%;
  z-index: 999;
}
.pop-full {
  background-color: #fff;
  border-radius: 15rpx;
  margin:auto;
  width:70%;
  position: relative;
  overflow: visible;
}
.pop-full-close {
  position: absolute;
  top: -75rpx;
  right: 47%;
  width: 62rpx;
  height: 62rpx;
  border-radius: 50%;
  background-color: transparent;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100%;
}
.pop-full .pop-content {
  overflow: auto;
  background-color: #fff;
  border-radius: 15rpx;
  -webkit-overflow-scrolling: touch;
}
.pop-full .pop-content .title{
  margin: 0rem;
  padding: 30rpx 0rem;
  text-align: center;
  border-bottom: 1rpx solid #d7d7d7;
}
.pop-full .pop-content .title p.active{
  border-bottom: solid 1 #f4393c;
}
.pop-full .pop-content .content {
  padding:1.5rem 0rem;
  margin: 0rem;
}
.pop-full .pop-content .content {
  padding:30rpx 20rpx;
  color:#333;
}
.pop-full .pop-content .content .wxParse-p {
  overflow:hidden;
}
.priceDetail {
  background-color: #fff;
  width: 100%;
}
.priceDetail .price-group{
  padding:  20rpx 30rpx;
  border-bottom: 1rpx solid #d7d7d7;
}
.div_mulLine .pop-full{
  width: 80%;
}
.div_mulLine .main-con{
    margin:  24rpx 30rpx;
}
.div_mulLine .main-con .t-title{
    font-size: 26rpx;
    padding: 20rpx 0;
}
.div_mulLine .main-con .line{
    height: 24rpx;
    width: 5rpx;
    margin: 0 12rpx;
    display: inline-block;
}
.div_mulLine .main-con .line-type{
    font-size: 24rpx;
    border-radius: 7rpx;
    padding: 3rpx 7rpx;
    margin-left: 24rpx;
}
.div_mulLine .div-flight{
    margin: -12rpx 0 12rpx 0;
}   
.endorseModal .title{
    border-bottom: solid 1px #d7d7d7;
    position: relative;
}
.endorseModal .title .text{
    text-align: center;
    padding:  36rpx 0;
}
.endorseModal .title .text.active{
    border-bottom: solid 5rpx #f4393c;
    opacity: 1 !important;
}